                                input.form-control.input-sm.bg-light.no-border.rounded.padder.ng-pristine.ng-valid.ng-touched(type='text', ng-model='consultantUser', placeholder='Consultant UserName', style='background-color: rgb(217, 227, 230);', aria-autocomplete='list', aria-expanded='false', aria-owns='typeahead-5-6521')
                                span.input-group-btn
                                  button.btn.btn-sm.bg-light.rounded(type='submit', style='background-color:#D9E3E6')
                                    i.fa.fa-search
                            div(ng-repeat='(index, consultant_username_sf_facet) in facet_counts.facet_fields.consultant_username_sf | filter:consultantUser | limitTo:20 track by $index', ng-if='$even')
