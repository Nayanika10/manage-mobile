                                  button.btn.btn-sm.bg-light.rounded(type='submit', style='background-color:#D9E3E6')
                                    i.fa.fa-search
                            div(ng-repeat="priority in ['High Priority', 'Open', 'Hold',' Closed'] | filter:jobstatus", ng-show='sideb.showPriorityFacet')
