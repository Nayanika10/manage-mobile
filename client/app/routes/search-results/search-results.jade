                                span(ng-click="query.consultant_username_sf[consultant_username_sf_facet] = !query.consultant_username_sf[consultant_username_sf_facet]") {{consultant_username_sf_facet | capitalize}} ({{facet_counts.facet_fields.consultant_username_sf[index+1]}})


