                                span(ng-click="query.state_id[+state_facet] = !query.state_id[+state_facet]") {{states[+state_facet]}} ({{facet_counts.facet_fields.state_id[index+1]}})

                        .panel.panel-info.no-border
                          .panel-heading-info.panel-heading(ng-click='sideb.showRegionFacet = !sideb.showRegionFacet')
                            span Location
                            i.fa.pull-right(ng-class="sideb.showRegionFacet?'fa-angle-down':'fa-angle-right'")
                            i.fa.fa-refresh.pull-right(ng-click='query.exp_location = {}', ng-show='sideb.showRegionFacet')
                          .panel-body.m-b-md(ng-show='sideb.showRegionFacet')
                            .form-group.w-full
                              .input-group.w-full
                                input.form-control.input-sm.bg-light.no-border.rounded.padder.ng-pristine.ng-valid.ng-touched(type='text', ng-model='location', placeholder='Location', style='background-color: rgb(217, 227, 230);', aria-autocomplete='list', aria-expanded='false', aria-owns='typeahead-5-6521')
                                span.input-group-btn
                                  button.btn.btn-sm.bg-light.rounded(type='submit', style='background-color:#D9E3E6')
                                    i.fa.fa-search
                            div(ng-repeat='(index, location_facet) in facet_counts.facet_fields.exp_location | filter:location | limitTo:20 track by $index', ng-if='$even')
                              span.text-ellipsis(title="{{location_facet}}({{facet_counts.facet_fields.exp_location[index+1]}})" style="margin-bottom: 5px;")
                                input(type="checkbox"  id="location_{{$index}}"  ng-model='query.exp_location[location_facet]')
                                span(ng-click="query.exp_location[location_facet] = !query.exp_location[location_facet] ") {{location_facet | capitalize}} ({{facet_counts.facet_fields.exp_location[index+1]}})


