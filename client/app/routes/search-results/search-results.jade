      .container(ng-repeat="applicant in applicants")
        .panel.panel-default
          .panel-heading
            p
              a.text-ellipsis(ui-sref='applicant.view({applicantId: applicant.id })') {{applicant.name | capitalize}}
          .panel-body
            p
              span(ng-bind="applicant._root_.client_name + '-' + applicant._root_.recruiter_username ")
