              span( ng-bind="applicant.client_name + '-' + applicant.consultant_username")
            p
              span(ng-bind="applicant[filter]" )


          .container
            ul.nav.nav-tabs
              li.active
                a(data-toggle='tab',href='#Comment',ng-click="$event.preventDefault();")
                  span.fa.fa-commenting

              li
                a(data-toggle='tab', href='#Time',ng-click="$event.preventDefault();")
                  span.fa.fa-clock-o

            .container

              h4
                span.label.label-primary  {{applicant.state_name}}



            .tab-content
              #Comment.tab-pane.fade.in.active
                p(ng-bind="applicant.latest_comment")


              #Time.tab-pane.fade
                p(ng-bind="moment(applicant.interview_time).format('YYYY-MM-DD')")

